<div class="markdown-body">
    <div class="markdown-heading">
        <h1 class="heading-element">
            {{page.title}}
        </h1>

        <a id="h1-{{page.id}}" class="anchor" href="#h1-{{page.id}}">
            <svg class="octicon" width="16" height="16" role="img" aria-hidden="true">
                <use href="#link-16"></use>
            </svg>
        </a>
    </div>
</div>

<div class="plugin-body">
    <div class="b-markguide-icons">
        {{#icons}}
        <div class="b-markguide-icons__item">
            <div class="b-markguide-icon">
                <div class="b-markguide-icon__data">
                    <pre>width: <b>{{width}}</b> height: <b>{{height}}</b> viewBox: <b>0 0 {{viewBoxWidth}} {{viewBoxHeight}}</b></pre>
                </div>
                <div class="b-markguide-icon__svg" style="max-width: 64px; height: 64px;">
                    {{{svg}}}

                    <div class="b-markguide-icon__grid">
                        <div class="b-markguide-icon__grid-top"></div>
                        <div class="b-markguide-icon__grid-bottom"></div>
                        <div class="b-markguide-icon__grid-left"></div>
                        <div class="b-markguide-icon__grid-right"></div>
                    </div>
                </div>
                <div class="b-markguide-icon__font" style="">

                </div>
                <div class="b-markguide-icon__name">
                    {{fileName}}
                </div>
                <div class="b-markguide-icon__code">
                    <code class="hljs language-none">@include icon({{name}}) {}</code>
                </div>
            </div>
        </div>
        {{/icons}}
    </div>
</div>