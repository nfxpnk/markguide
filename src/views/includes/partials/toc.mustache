<div class="markguide-toc">
    {{#src}}
    <div class="markguide-toc__info">
        <div class="markguide-toc__path"><strong>Path:</strong> {{src}}</div>
    </div>
    {{/src}}
    {{#content.toc}}
        <div class="markguide-toc__item" role="listitem">
            <a class="markguide-toc__ln markguide-toc__ln_{{level}}" href="#h{{level}}-{{escapedText}}">
                {{{text}}}
            </a>
        </div>
    {{/content.toc}}
</div>
