{
    "guideSrc": "_example/scss-source/",
    "guideDest": "_example/markguide-static/",
    "cssSrc": "assets/css/",
    "projectCompiledFiles": "_example/projects-static-files/",
    "projectStyles": [
        "styles"
    ],
    "projectScripts": [
        "main"
    ],
    "internalAssetsPath": "assets",
    "excludedSassFiles": "_dev-markguide-settings|_dev-markguide-definitions",
    "customPluginsPath": "_example/custom-plugins/",
    "enabledPlugins": [
        {
            "name": "icons",
            "options": {
                "iconsFolder": "icons",
                "fileExtension": "svg"
            }
        },
        {
            "name": "colors",
            "options": {
                "filePath": "configuration/_colors.scss"
            }
        },
        {
            "name": "typography",
            "options": {}
        },
        {
            "name": "dummy",
            "options": {
                "filePath": "file.html"
            }
        }
    ],
    "projectFonts": [
        {
            "filename": "roboto-mono-v22-latin-regular.woff2",
            "properties": {
                "family": "Roboto",
                "style": "normal",
                "weight": "400"
            }
        },
        {
            "filename": "icons.woff2",
            "properties": {
                "family": "icons",
                "style": "normal",
                "weight": "400"
            }
        }
    ]
}
